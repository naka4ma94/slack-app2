<%= form_with(model: @room_user, local: true) do |form| %>
  <div class="field">
    <%= form.hidden_field :user_id, value: current_user.id %>
  </div>

  <div class="field">
    <%= form.hidden_field :room_id, value: @workspace.id %>
  </div>

  <div class="field">
    <%= form.submit "はい" %>
  </div>
<% end %>